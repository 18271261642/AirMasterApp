<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:background="@color/play_color_theme"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent">

  <com.app.airmaster.widget.CommTitleView
      android:layout_width="match_parent"
      android:id="@+id/sysSettingTitleView"
      android:layout_height="wrap_content"/>


    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_20"
        android:layout_marginStart="@dimen/dp_10"
        android:layout_marginEnd="@dimen/dp_10"
        android:scrollbars="none">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">



        <com.bonlala.widget.layout.SettingBar
            android:id="@+id/sysGasBar"
            android:layout_width="match_parent"
            app:bar_leftDrawable="@mipmap/ic_sys_gas_pressure"
            app:bar_leftText="@string/string_set_tank_pressure"
            android:background="@color/transparent"
            app:bar_leftTextColor="@color/white"
            app:bar_rightDrawable="@mipmap/ic_set_right_back"
            app:bar_lineVisible="false"
            android:layout_height="wrap_content"/>


        <com.bonlala.widget.layout.SettingBar
            android:id="@+id/sysMemoryModelBar"
            android:layout_width="match_parent"
            app:bar_leftDrawable="@mipmap/ic_sys_memory_model"
            app:bar_leftText="@string/string_car_memory_model"
            android:background="@color/transparent"
            app:bar_leftTextColor="@color/white"
            app:bar_rightDrawable="@mipmap/ic_set_right_back"
            app:bar_lineVisible="false"
            android:layout_height="wrap_content"/>




        <com.bonlala.widget.layout.SettingBar
            android:id="@+id/sysWorkModelBar"
            android:visibility="gone"
            android:layout_width="match_parent"
            app:bar_leftDrawable="@mipmap/ic_sys_work_model"
            app:bar_leftText="@string/string_car_work_model"
            android:background="@color/transparent"
            app:bar_leftTextColor="@color/white"
            app:bar_rightDrawable="@mipmap/ic_set_right_back"
            app:bar_lineVisible="false"
            android:layout_height="wrap_content"/>


        <com.bonlala.widget.layout.SettingBar
            android:id="@+id/sysScaleSettingBar"
            android:layout_width="match_parent"
            app:bar_leftDrawable="@mipmap/ic_sys_height_scale"
            app:bar_leftText="@string/string_set_sensor_tool"
            android:background="@color/transparent"
            app:bar_leftTextColor="@color/white"
            app:bar_rightDrawable="@mipmap/ic_set_right_back"
            app:bar_lineVisible="false"
            android:layout_height="wrap_content"/>


        <com.bonlala.widget.layout.SettingBar
            android:id="@+id/sysPowerProtectBar"
            android:layout_width="match_parent"
            app:bar_leftDrawable="@mipmap/ic_sys_power_protect"
            app:bar_leftText="@string/string_set_battery_protect"
            android:background="@color/transparent"
            app:bar_leftTextColor="@color/white"
            app:bar_rightDrawable="@mipmap/ic_set_right_back"
            app:bar_lineVisible="false"
            android:layout_height="wrap_content"/>


        <com.bonlala.widget.layout.SettingBar
            android:id="@+id/sysIgnitionBar"
            android:layout_width="match_parent"
            app:bar_leftDrawable="@mipmap/ic_sys_ignition"
            app:bar_leftText="@string/string_set_start_end"
            android:background="@color/transparent"
            app:bar_leftTextColor="@color/white"
            app:bar_rightDrawable="@mipmap/ic_set_right_back"
            app:bar_lineVisible="false"
            android:layout_height="wrap_content"/>


        <com.bonlala.widget.layout.SettingBar
            android:id="@+id/carSysHeightMemoryBar"
            android:layout_width="match_parent"
            app:bar_leftDrawable="@mipmap/ic_sys_height_memory"
            app:bar_leftText="@string/string_set_preset_setting"
            android:background="@color/transparent"
            app:bar_leftTextColor="@color/white"
            app:bar_rightDrawable="@mipmap/ic_set_right_back"
            app:bar_lineVisible="false"
            android:layout_height="wrap_content"/>


        <com.bonlala.widget.layout.SettingBar
            android:id="@+id/sysRepairBar"
            android:layout_width="match_parent"
            app:bar_leftDrawable="@mipmap/ic_sys_repair_model"
            app:bar_leftText="@string/string_set_service_model"
            android:background="@color/transparent"
            app:bar_leftTextColor="@color/white"
            app:bar_rightDrawable="@mipmap/ic_set_right_back"
            app:bar_lineVisible="false"
            android:layout_height="wrap_content"/>


        <com.bonlala.widget.layout.SettingBar
            android:id="@+id/sysLowestBar"
            android:layout_width="match_parent"
            app:bar_leftDrawable="@mipmap/ic_sys_low_air"
            app:bar_leftText="@string/string_set_minimum_dirve"
            android:background="@color/transparent"
            app:bar_leftTextColor="@color/white"
            app:bar_rightDrawable="@mipmap/ic_set_right_back"
            app:bar_lineVisible="false"
            android:layout_height="wrap_content"/>

        <com.bonlala.widget.layout.SettingBar
            android:id="@+id/sysAirBalanceBar"
            android:layout_width="match_parent"
            app:bar_leftDrawable="@mipmap/ic_sys_low_air"
            app:bar_leftText="@string/string_set_ap_balance"
            android:background="@color/transparent"
            app:bar_leftTextColor="@color/white"
            app:bar_rightDrawable="@mipmap/ic_set_right_back"
            app:bar_lineVisible="false"
            android:layout_height="wrap_content"/>

    </LinearLayout>
    </androidx.core.widget.NestedScrollView>

</LinearLayout>